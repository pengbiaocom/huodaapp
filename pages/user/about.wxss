/* pages/user/about.wxss */
.img-view{
  width: 100%;
  height: 320rpx;
  line-height: 320rpx;
  text-align: center;
}

.images{
  width: 180rpx;
  height: 180rpx;
  vertical-align: middle;
  background: #fff;
  border-radius: 5px;
}

.list{
  padding-left: 30rpx;
  padding-right: 30rpx;
  height: 80rpx;
  line-height: 80rpx;
  background: #fff;
  border-bottom: 1px solid #D9D9D9;
}
.list .left{
  float: left;
  width: 50%;
  font-size: 28rpx;
}
.list .right{
  float: left;
  width: 50%;
  font-size: 28rpx;
  color: #999999;
  text-align: right;
}

.last{border:none}